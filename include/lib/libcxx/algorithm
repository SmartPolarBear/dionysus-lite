#if !defined(__INCLUDE_LIBCXX_ALGORITHM)
#define __INCLUDE_LIBCXX_ALGORITHM

namespace sysstd
{

template <typename T>
constexpr const T &max(const T &a, const T &b)
{
    return (a < b) ? b : a;
}

template <class T>
constexpr const T &min(const T &a, const T &b)
{
    return (b < a) ? b : a;
}

} // namespace sysstd

#endif // __INCLUDE_LIBCXX_ALGORITHM
